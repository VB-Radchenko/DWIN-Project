
//==============================================================================
//---------------------------------Includes-------------------------------------
//==============================================================================
#include "DWIN_APP.h"
#include "DWIN.h"
#include <stdbool.h>
//==============================================================================
//---------------------------------Defines--------------------------------------
//==============================================================================
#define ledDataAdress 0x5006
//==============================================================================
//--------------------------------Variables-------------------------------------
//==============================================================================
extern struct readDataDWIN_P readDataDWIN;
uint16_t PWMData = 0;
//==============================================================================
//--------------------------------PROTOTYPE-------------------------------------
//==============================================================================


//==============================================================================
//--------------------------------FUNCTIONS-------------------------------------
//==============================================================================
void ledPWM()
{
	parsingDWIN();
	readVariableDWIN(0x5006, 1);
	parsingDWIN();
	if ((readDataDWIN.parsingDataDWIN.data[0] == (uint8_t)(ledDataAdress && 0x00FF)>>8) && (readDataDWIN.parsingDataDWIN.data[1] == (uint8_t)(ledDataAdress && 0x00FF))) // Checking for address match
	{
		PWMData = readDataDWIN.parsingDataDWIN.data[2]<<8 | readDataDWIN.parsingDataDWIN.data[3];
	}
}

//==============================================================================
//---------------------------------END FILE-------------------------------------
//==============================================================================
