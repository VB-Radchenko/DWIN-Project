
//==============================================================================
//---------------------------------Includes-------------------------------------
//==============================================================================
#include "DWIN_APP.h"
#include "DWIN.h"
#include <stdbool.h>
//==============================================================================
//---------------------------------Defines--------------------------------------
//==============================================================================
#define ledDataAdress 0x5006
//==============================================================================
//--------------------------------Variables-------------------------------------
//==============================================================================
extern struct readDataDWIN_P readDataDWIN;
uint16_t PWMData = 0;
//==============================================================================
//--------------------------------PROTOTYPE-------------------------------------
//==============================================================================
uint16_t adress = 0;
uint8_t adress_h,adress_l;
//==============================================================================
//--------------------------------FUNCTIONS-------------------------------------
//==============================================================================
void ledPWM()
{
	parsingDWIN();
	readVariableDWIN(0x5006, 1);
	parsingDWIN();
	adress = ledDataAdress;
	adress_h = ((ledDataAdress & 0xFF00)>>8);
	adress_l = ledDataAdress & 0x00FF;
	if ((readDataDWIN.parsingDataDWIN.data[0] == ((ledDataAdress & 0xFF00)>>8)) && (readDataDWIN.parsingDataDWIN.data[1] == ledDataAdress & 0x00FF)) // Checking for address match
	{
		PWMData = readDataDWIN.parsingDataDWIN.data[2]<<8 | readDataDWIN.parsingDataDWIN.data[3];
	}
}

//==============================================================================
//---------------------------------END FILE-------------------------------------
//==============================================================================
